config WIFI_SSID
    string "WiFi SSID to connect to"
    help
        The WiFi SSID that the board will connect to. 

config WIFI_SECURITY
    int "wifi.h wifi_security_type"
    default 0
    help
        "see zephyr/net/wifi.h enum wifi_security_type. Default 0 (no security)"

menu "Zephyr Kernel"
source "Kconfig.zephyr"
endmenu

